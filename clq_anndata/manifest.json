{
  "name": "clq_anndata",
  "description": "Run CLQ on spatial graph",
  "stage": "1",
  "params":{
    "radius": {
      "name": "radius",
      "description": "The radius used to construct the neighborhood content vector, in microns",
      "type": "float",
      "required": true,
      "default": 50
    },
    "n_perms": {
      "name": "n_perms",
      "description": "The number of permutations to use for statistical testing",
      "type": "int",
      "required": true,
      "default": 100
    }
  },
  "script_path": "clq_anndata",
  "return": {
    "CLQ": {
      "name": "CLQ",
      "type": "adata",
      "description": "h5ad with CLQ vectors"
    }
  },
  "depends_and_script": [
    "clustering",
    "rename_clusters"
  ],
  "conda": ["python=3.10"],
  "conda_pip": [
    "leidenalg==0.8.10",
    "squidpy==1.3.0",
    "pandas==1.5.3",
    "scanpy==1.9.4",
    "anndata==0.10.9",
    "llvmlite==0.42.0",
    "matplotlib==3.7.2",
    "dask==2022.2.0"
  ],
  "libs": [
    "cmake",
    "numpy==1.23.4"
  ]
}
